{
  "name": "bookshelf-slug",
  "version": "1.0.1",
  "description": "Bookshelfjs plugin to generate unique slugs",
  "main": "build/plugin.js",
  "scripts": {
    "mocha": "mocha -r ts-node/register --file ./test/hooks/teardown.ts --file ./test/hooks/setup.ts",
    "test": "npm run mocha 'test/**/*.spec.ts'",
    "build": "rimraf build && tsc -p tsconfig.build.json",
    "knex": "knex --knexfile ./test/database/knexfile.ts",
    "migrate": "npm run knex -- migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oscaroox/bookshelf-slug.git"
  },
  "bugs": {
    "url": "https://github.com/oscaroox/bookshelf-slug/issues"
  },
  "keywords": [
    "bookshelf",
    "bookshelf-plugin",
    "slug",
    "bookshelf-slug",
    "orm"
  ],
  "author": "oscar druiventak",
  "license": "MIT",
  "devDependencies": {
    "@types/bookshelf": "^1.1.1",
    "@types/chai": "^4.2.11",
    "@types/faker": "^4.1.12",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.14",
    "bluebird": "^3.4.1",
    "bookshelf": "^1.0.0",
    "chai": "^4.1.2",
    "faker": "^4.1.0",
    "knex": "^0.21.1",
    "mocha": "^9.0.1",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "slugify": "^1.4.2"
  },
  "peerDependencies": {
    "bookshelf": ">= 1.0.0 < 1.3"
  }
}
